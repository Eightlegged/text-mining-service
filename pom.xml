		    <version>1.5.4</version>
		</dependency>
		<dependency>
		    <groupId>kr.bydelta</groupId>
		    <artifactId>koalanlp-server_2.11</artifactId>
		    <version>1.5.4</version>
		</dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
			</plugin>
		</plugins>
	</build>


</project>
