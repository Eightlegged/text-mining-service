		    <artifactId>koalanlp-core_2.12</artifactId>
		    <version>1.5.4</version>
		</dependency>
		<dependency>
		    <groupId>kr.bydelta</groupId>
		    <artifactId>koalanlp-hannanum_2.12</artifactId>
		    <version>1.5.4</version>
